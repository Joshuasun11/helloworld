#define _CRT_SECURE_NO_WARNINGS 
#include <cstdio> //标准输入输出函数 
#include <cmath> 
#include <cstring> //字符串处理函数 
int main() {
	printf("hello\n");
	double x = 3.14;
	printf("%lf %lf\n", sqrt(x), sin(x));

	char s[10] = "hello";
	puts(s);
	char s2[16];
	strcpy(s2, "world");
	puts(s2);
	strcat(s2, "sdfsdf");
	puts(s2);
	printf("%d %d\n", strlen(s), strlen(s2));
	return 0;
}
